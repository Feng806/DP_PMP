MATLAB_ROOT = C:\PROGRA~1\MATLAB\R2015a
MAKEFILE = myxcorr_mex.mk

include myxcorr_mex.mki


SRC_FILES =  \
	myxcorr_data.c \
	myxcorr_initialize.c \
	myxcorr_terminate.c \
	myxcorr.c \
	norm.c \
	_coder_myxcorr_api.c \
	_coder_myxcorr_mex.c \
	_coder_myxcorr_info.c

MEX_FILE_NAME_WO_EXT = myxcorr
MEX_FILE_NAME = $(MEX_FILE_NAME_WO_EXT).mexw64
TARGET = $(MEX_FILE_NAME)

SYS_LIBS = 


#
#====================================================================
